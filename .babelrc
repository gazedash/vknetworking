{
  "presets": [
    [
      "es2015",
      {
        "modules": false
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    "syntax-flow",
    "transform-flow-strip-types"
  ],
  "comments": false,
  "env": {
    "development": {
      "plugins": [
        "tcomb"
      ]
    },
    "test": {
      "plugins": [
        "istanbul"
      ]
    }
  }
}
